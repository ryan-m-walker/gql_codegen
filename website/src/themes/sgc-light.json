{
  "name": "sgc-light",
  "type": "light",
  "fg": "#171717",
  "bg": "#f5f5f5",
  "colors": {
    "editor.background": "#f5f5f5",
    "editor.foreground": "#171717"
  },
  "tokenColors": [
    {
      "settings": {
        "foreground": "#171717",
        "background": "#f5f5f5"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator.expression",
        "storage",
        "storage.type",
        "storage.modifier",
        "keyword.operator.new",
        "keyword.operator.logical",
        "variable.language.this",
        "variable.language.super"
      ],
      "settings": {
        "foreground": "#b45309"
      }
    },
    {
      "scope": [
        "string",
        "string.quoted",
        "string.template"
      ],
      "settings": {
        "foreground": "#525252"
      }
    },
    {
      "scope": [
        "comment",
        "comment.line",
        "comment.block",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#a3a3a3",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language"
      ],
      "settings": {
        "foreground": "#737373"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#b45309"
      }
    },
    {
      "scope": [
        "variable",
        "variable.other",
        "entity.name.function",
        "support.function",
        "meta.function-call"
      ],
      "settings": {
        "foreground": "#171717"
      }
    },
    {
      "scope": [
        "punctuation",
        "meta.brace",
        "punctuation.definition.block",
        "punctuation.separator",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#525252"
      }
    },
    {
      "scope": [
        "keyword.operator",
        "keyword.operator.assignment",
        "keyword.operator.comparison",
        "keyword.operator.arithmetic"
      ],
      "settings": {
        "foreground": "#525252"
      }
    },
    {
      "scope": [
        "entity.name.tag",
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#b45309"
      }
    },
    {
      "scope": [
        "variable.other.property",
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#171717"
      }
    }
  ]
}
